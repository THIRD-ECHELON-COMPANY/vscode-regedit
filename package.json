{
	"name": "regedit",
	"description": "A registry editor inside VS Code",
	"version": "1.0.0",
	"icon": "assets/main-icon.png",
	"publisher": "m417z",
	"engines": {
		"vscode": "^1.47.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:regedit.start",
		"onWebviewPanel:regedit"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/m417z/vscode-regedit"
	},
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "regedit.start",
				"title": "%commands.start.title%",
				"category": "%commands.category%"
			}
		],
		"menus": {
			"editor/title": [
				{
					"command": "regedit.start"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -w -p ./"
	},
	"dependencies": {
		"native-reg": "^0.3.4",
		"vscode-nls-i18n": "github:m417z/vscode-nls-i18n#dist"
	},
	"devDependencies": {
		"@typescript-eslint/eslint-plugin": "^3.0.2",
		"@typescript-eslint/parser": "^3.0.2",
		"eslint": "^7.1.0",
		"typescript": "^4.0.2",
		"@types/vscode": "^1.47.0",
		"@types/node": "^12.12.0"
	}
}
